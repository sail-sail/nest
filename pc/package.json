{
  "name": "pc",
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "preview": "set NODE_OPTIONS=--max-old-space-size=4096 && vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build-prod": "set NODE_OPTIONS=--max-old-space-size=4096 && vite build --emptyOutDir && node src/utils/postbuild.js",
    "build-test": "set NODE_OPTIONS=--max-old-space-size=4096 && vite build --mode test --emptyOutDir && node src/utils/postbuild.js",
    "permit": "node src/utils/permit_scan.js",
    "rsw:build": "rsw && npm run build",
    "typecheck": "set NODE_OPTIONS=--max-old-space-size=4096 && vue-tsc --noEmit --skipLibCheck && eslint .",
    "lint": "eslint .",
    "lint-staged": "lint-staged",
    "codegen": "cd .. && npm run codegen",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "node src/utils/postinstall.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.3.2",
    "@formkit/auto-animate": "0.9.0",
    "@jsquash/resize": "2.1.0",
    "@jsquash/webp": "1.5.0",
    "@opentiny/fluent-editor": "3.25.4",
    "@qr-x/vue": "1.1.1",
    "@vue-office/docx": "1.6.3",
    "@vue-office/excel": "1.7.14",
    "@vue/reactivity": "3.5.22",
    "@vue/runtime-dom": "3.5.22",
    "@vue/shared": "3.5.22",
    "@vueuse/core": "13.9.0",
    "cronstrue": "3.3.0",
    "danmu": "0.18.1",
    "dayjs": "1.11.18",
    "decimal.js": "10.6.0",
    "ejsexcel-browserify": "4.4.3",
    "element-plus": "2.11.4",
    "file-saver": "2.0.5",
    "filesize": "11.0.13",
    "graphql": "16.11.0",
    "graphql-combine-query": "1.2.4",
    "jszip": "3.10.1",
    "numeral": "2.0.6",
    "pinyin-pro": "3.27.0",
    "sortablejs": "1.15.6",
    "swiper": "12.0.2",
    "vue": "3.5.22",
    "vue-countup-v3": "1.4.2",
    "vue-demi": "0.14.10",
    "vue-router": "4.6.3",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "@plugin-web-update-notification/vite": "2.0.2",
    "@types/file-saver": "2.0.7",
    "@types/node": "24.8.0",
    "@types/sortablejs": "1.15.8",
    "@unocss/preset-rem-to-px": "66.5.4",
    "@vitejs/plugin-vue": "6.0.1",
    "@vitejs/plugin-vue-jsx": "5.1.1",
    "@vue-macros/eslint-config": "3.1.1",
    "@vue-macros/reactivity-transform": "3.1.1",
    "@vue/compiler-sfc": "3.5.22",
    "@vue/devtools-api": "8.0.2",
    "@vue/runtime-core": "3.5.22",
    "dotenv": "17.2.3",
    "eslint": "9.37.0",
    "eslint-plugin-vue": "10.5.1",
    "globals": "16.4.0",
    "mysql2": "3.15.2",
    "sass": "1.93.2",
    "spark-md5": "3.0.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.1",
    "unocss": "66.5.4",
    "unplugin-auto-import": "20.2.0",
    "unplugin-icons": "22.4.2",
    "unplugin-turbo-console": "2.2.0",
    "unplugin-vue-components": "29.1.0",
    "unplugin-vue-define-options": "3.1.1",
    "vite": "7.1.10",
    "vite-plugin-devtools-json": "1.0.0",
    "vite-plugin-inspect": "11.3.3",
    "vite-plugin-pwa": "1.1.0",
    "vite-plugin-vue-devtools": "8.0.2",
    "vite-plugin-vue-inspector": "5.3.2",
    "vite-plugin-wasm": "3.5.0",
    "vitest": "3.2.4",
    "vue-tsc": "3.1.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@vue-office/docx",
      "@vue-office/excel",
      "esbuild",
      "vue-demi"
    ]
  }
}
