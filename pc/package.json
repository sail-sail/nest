{
  "name": "pc",
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "preview": "set NODE_OPTIONS=--max-old-space-size=4096 && vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build-prod": "set NODE_OPTIONS=--max-old-space-size=4096 && vite build --emptyOutDir && node src/utils/postbuild.js",
    "build-test": "set NODE_OPTIONS=--max-old-space-size=4096 && vite build --mode test --emptyOutDir && node src/utils/postbuild.js",
    "permit": "node src/utils/permit_scan.js",
    "rsw:build": "rsw && npm run build",
    "typecheck": "set NODE_OPTIONS=--max-old-space-size=4096 && vue-tsc --noEmit --skipLibCheck && eslint .",
    "lint": "eslint .",
    "lint-staged": "lint-staged",
    "codegen": "cd .. && npm run codegen",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "node src/utils/postinstall.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.3.1",
    "@formkit/auto-animate": "0.8.2",
    "@jsquash/resize": "2.1.0",
    "@jsquash/webp": "1.4.0",
    "@opentiny/fluent-editor": "3.25.4",
    "@qr-x/vue": "1.1.1",
    "@vue-office/docx": "1.6.3",
    "@vue-office/excel": "1.7.14",
    "@vue/reactivity": "3.5.13",
    "@vue/runtime-dom": "3.5.13",
    "@vue/shared": "3.5.13",
    "@vueuse/core": "13.1.0",
    "cronstrue": "2.60.0",
    "danmu": "0.15.1",
    "dayjs": "1.11.13",
    "decimal.js": "10.5.0",
    "ejsexcel-browserify": "4.4.3",
    "element-plus": "2.9.9",
    "file-saver": "2.0.5",
    "filesize": "10.1.6",
    "graphql": "16.11.0",
    "graphql-combine-query": "1.2.4",
    "jszip": "3.10.1",
    "numeral": "2.0.6",
    "pinia": "3.0.2",
    "pinia-plugin-persistedstate": "4.2.0",
    "pinyin-pro": "3.26.0",
    "sortablejs": "1.15.6",
    "swiper": "11.2.6",
    "vue": "3.5.13",
    "vue-countup-v3": "1.4.2",
    "vue-demi": "0.14.10",
    "vue-router": "4.5.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@eslint/js": "9.26.0",
    "@plugin-web-update-notification/vite": "2.0.0",
    "@types/file-saver": "2.0.7",
    "@types/node": "22.15.9",
    "@types/sortablejs": "1.15.8",
    "@unocss/preset-rem-to-px": "66.1.0",
    "@vitejs/plugin-vue": "5.2.3",
    "@vitejs/plugin-vue-jsx": "4.1.2",
    "@vue-macros/eslint-config": "3.0.0-beta.10",
    "@vue-macros/reactivity-transform": "3.0.0-beta.10",
    "@vue/compiler-sfc": "3.5.13",
    "@vue/runtime-core": "3.5.13",
    "dotenv": "16.5.0",
    "eslint": "9.26.0",
    "eslint-plugin-vue": "10.1.0",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "mysql2": "3.14.1",
    "sass": "1.87.0",
    "spark-md5": "3.0.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.0",
    "unocss": "66.1.0",
    "unplugin-auto-import": "19.1.2",
    "unplugin-icons": "22.1.0",
    "unplugin-turbo-console": "2.1.3",
    "unplugin-vue-components": "28.5.0",
    "unplugin-vue-define-options": "3.0.0-beta.10",
    "vite": "6.3.5",
    "vite-plugin-inspect": "11.0.1",
    "vite-plugin-pwa": "1.0.0",
    "vite-plugin-vue-devtools": "7.7.6",
    "vite-plugin-vue-inspector": "5.3.1",
    "vite-plugin-wasm": "3.4.1",
    "vitest": "3.1.3",
    "vue-tsc": "2.2.10"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "npm run typecheck"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@vue-office/docx",
      "@vue-office/excel",
      "esbuild",
      "vue-demi"
    ]
  }
}
