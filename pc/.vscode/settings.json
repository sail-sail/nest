{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.swc": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/tsconfig.json": true,
    "**/.editorconfig": true,
    "**/protractor.conf.js": true,
    "**/tslint.json": true,
    "**/karma.conf.js": true,
    "**/.gitignore": true,
    "**/.gitkeep": true,
    "**/LICENSE": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/node_modules/**": true,
    "**/__unconfig_vite.config.ts": true,
    ".eslintrc-auto-import.json": true,
    "**/vite.config.mts.timestamp-*.mjs": true,
    "**/.npmrc": true,
    "public/ejsexcel.min.js": true,
    "**/.husky": true,
    "graphql.config.js": true,
    "uno.config.ts": true
  },
  "search.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.swc": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/tsconfig.json": true,
    "**/.editorconfig": true,
    "**/protractor.conf.js": true,
    "**/tslint.json": true,
    "**/karma.conf.js": true,
    "**/.gitignore": true,
    "**/.gitkeep": true,
    "**/LICENSE": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/node_modules/**": true,
    "**/__unconfig_vite.config.ts": true,
    ".eslintrc-auto-import.json": true,
    "**/vite.config.mts.timestamp-*.mjs": true,
    "**/.npmrc": true,
    "public/ejsexcel.min.js": true,
    "**/.husky": true,
    "graphql.config.js": true,
    "uno.config.ts": true
  },
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.git/logs/**": true,
    "dist/**": true,
    "dist-ssr/**": true,
    "build/**": true,
    ".cache/**": true,
    ".temp/**": true,
    ".tmp/**": true,
    "**/*.log": true,
    ".eslintrc-auto-import.json": true,
    "**/vite.config.mts.timestamp-*.mjs": true,
    "public/ejsexcel.min.js": true,
    "public/@jsquash/**": true,
    "public/excel_template/**/*.xlsx": true,
    "public/import_template/**/*.xlsx": true,
    "**/.rsw/**": true,
    "**/@rsw/**": true,
    "**/.pnpm-store/**": true
  },
  "diffEditor.useInlineViewWhenSpaceIsLimited": false,
  "git.timeline.showUncommitted": true,
  "git.blame.editorDecoration.template": "${subject}, ${authorName} (${authorDateAgo})",
  "git.openRepositoryInParentFolders": "never",
  "git.autofetch": false,
  "git.autorefresh": true,
  "scm.showHistoryGraph": false,
  "deno.enable": false,
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "typescript.inlayHints.parameterTypes.enabled": false,
  "npm.registry": "https://registry.npmmirror.com",
  "npm.updateStrategy": "LATEST",
  "editor.quickSuggestionsDelay": 0,
  "typescript.suggest.autoImports": false,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "unocss.root": "",
  "unocss.colorPreview": false,
  "terminal.integrated.env.windows": {
    "VUE_EDITOR": "code"
  },
  "files.eol": "\n",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  }
}
