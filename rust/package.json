{
  "name": "rust",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "start": "cargo run",
    "gqlgen": "cargo run --bin schema && graphql-codegen --config ./generated/common/script/graphql_codegen_config.ts",
    "typecheck": "cargo clippy",
    "build-prod": "node ./generated/common/script/build.js --env prod",
    "build-test": "node ./generated/common/script/build.js --env test",
    "publish": "node ./generated/common/script/publish.js",
    "codegen": "cd .. && npm run codegen",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "6.0.0",
    "@graphql-codegen/typescript": "5.0.2",
    "@graphql-codegen/typescript-operations": "5.0.2",
    "@parcel/watcher": "2.5.1",
    "@swc/wasm": "1.13.20",
    "@types/fs-extra": "11.0.4",
    "@types/minimist": "1.2.5",
    "@types/node": "24.8.0",
    "commander": "14.0.1",
    "dotenv": "17.2.3",
    "fs-extra": "11.3.2",
    "graphql": "16.11.0",
    "graphql-ts-client-codegen": "3.1.17",
    "minimist": "1.2.8",
    "ssh2-promise": "1.0.3",
    "typescript": "5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "cpu-features",
      "ssh2"
    ]
  }
}
