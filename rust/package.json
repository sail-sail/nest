  "scripts": {
    "start": "cargo run",
    "gqlgen": "cargo run --bin schema && graphql-codegen --config ./src/common/script/graphql_codegen_config.ts",
    "typecheck": "cargo clippy",
    "build-prod": "node ./src/common/script/build.js --env prod && node ./src/common/script/publish.js --env prod",
    "build-test": "node ./src/common/script/build.js --env test && node ./src/common/script/publish.js --env test",
    "publish": "node ./src/common/script/publish.js",
    "codegen": "cd .. && npm run codegen",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "4.0.7",
    "@graphql-codegen/typescript-operations": "4.2.1",
    "@parcel/watcher": "2.4.1",
    "@swc/wasm": "1.6.0",
    "@types/fs-extra": "11.0.4",
    "@types/minimist": "1.2.5",
    "@types/node": "20.14.2",
    "chokidar": "3.6.0",
    "commander": "12.1.0",
    "dotenv": "16.4.5",
    "fs-extra": "11.2.0",
    "graphql": "16.8.2",
    "graphql-ts-client-codegen": "3.1.17",
    "minimist": "1.2.8",
    "ssh2-promise": "1.0.3",
    "ts-node": "10.9.2",
    "typescript": "5.4.5"
  }
}
