{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build-test": "nuxt build --dotenv .env.test",
    "build-prod": "nuxt build --dotenv .env.prod",
    "start": "nuxt dev",
    "typecheck": "nuxt typecheck && eslint .",
    "lint": "eslint .",
    "lint-staged": "lint-staged",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "decimal.js-light": "2.5.1",
    "file-saver": "2.0.5",
    "graphql": "16.10.0",
    "graphql-combine-query": "1.2.4",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.1.0",
    "@nuxt/icon": "1.10.3",
    "@types/file-saver": "2.0.7",
    "@types/node": "22.13.5",
    "@unocss/nuxt": "66.0.0",
    "@unocss/preset-rem-to-px": "66.0.0",
    "@unocss/transformer-directives": "66.0.0",
    "@vue-macros/nuxt": "1.13.2",
    "@vue-macros/reactivity-transform": "1.1.6",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "nuxt": "3.15.4",
    "sass": "1.85.0",
    "typescript": "5.7.3",
    "unocss": "66.0.0",
    "vue-tsc": "2.2.4"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "npm run typecheck"
    ]
  }
}
