{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build-test": "nuxt build --dotenv .env.test",
    "build-prod": "nuxt build --dotenv .env.prod",
    "start": "nuxt dev",
    "typecheck": "nuxt typecheck && eslint .",
    "lint": "eslint .",
    "lint-staged": "lint-staged",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "decimal.js-light": "2.5.1",
    "file-saver": "2.0.5",
    "graphql": "16.11.0",
    "graphql-combine-query": "1.2.4",
    "vue": "3.5.17",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.4.1",
    "@nuxt/icon": "1.13.0",
    "@types/file-saver": "2.0.7",
    "@types/node": "24.0.3",
    "@unocss/nuxt": "66.2.3",
    "@unocss/preset-rem-to-px": "66.2.3",
    "@unocss/transformer-directives": "66.2.3",
    "@vue-macros/nuxt": "3.0.0-beta.14",
    "@vue-macros/reactivity-transform": "3.0.0-beta.14",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "nuxt": "3.17.5",
    "sass": "1.89.2",
    "typescript": "5.8.3",
    "unocss": "66.2.3",
    "vue-tsc": "2.2.10"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "npm run typecheck"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver",
      "vue-demi"
    ]
  }
}
