  "version": "0.2.0",
  "configurations": [
    {
      "address": "127.0.0.1",
      "localRoot": "${workspaceFolder}/deno",
      "name": "deno",
      "port": 9229,
      "request": "attach",
      "type": "node"
    },
    {
      "type": "msedge",
      "name": "edge",
      "request": "launch",
      "runtimeArgs": [
        "--headless",
        "--remote-debugging-port=9222"
      ],
      "url": "http://localhost:4000"
    }
  ]
}
