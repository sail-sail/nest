{
  "name": "deno",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "start": "node lib/script/mother.js",
    "gqlgen": "npx graphql-codegen --config ./lib/script/graphql_codegen_config.yml",
    "build": "deno run -A --import-map=./import_map.json ./lib/script/build.ts --target linux",
    "publish": "node lib/script/publish.js",
    "build:win": "deno run -A --import-map=./import_map.json ./lib/script/build.ts",
    "codegen": "cd .. && npm run codegen",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.0",
    "@graphql-codegen/typescript": "4.0.0",
    "@graphql-codegen/typescript-operations": "4.0.0",
    "@types/node": "20.2.5",
    "chokidar": "3.5.3",
    "dotenv": "16.1.3",
    "graphql": "16.6.0",
    "graphql-ts-client-codegen": "3.1.14",
    "ssh2-promise": "1.0.3",
    "typescript": "5.0.4"
  }
}
