{
  "lock": false,
  "compilerOptions": {
    "strict": true,
    "allowUnreachableCode": true,
    "allowUnusedLabels": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "lib": [
      "esnext",
      "deno.window",
      "deno.ns"
    ],
    "types": [
      "./typings/types.ts"
    ]
  },
  "lint": {
    "rules": {
      "exclude": [
        "require-await",
        "no-unused-vars"
      ]
    },
    "include": [
      "src/"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ]
  },
  "npmRegistry": "https://registry.npmmirror.com",
  "imports": {
    "./": "./",
    "/": "./",
    "@hexagon/croner": "jsr:@hexagon/croner@9.0.0",
    "@oak/oak": "jsr:@oak/oak@17.1.4",
    "@std/assert": "jsr:@std/assert@1.0.12",
    "@std/crypto": "jsr:@std/crypto@1.0.4",
    "@std/dotenv": "jsr:@std/dotenv@0.225.3",
    "@std/encoding": "jsr:@std/encoding@1.0.8",
    "@std/fs": "jsr:@std/fs@1.0.15",
    "@std/io": "jsr:@std/io@0.225.2",
    "@std/log": "jsr:@std/log@0.224.14",
    "@std/path": "jsr:@std/path@1.0.8",
    "lru-cache": "npm:lru-cache@11.1.0",
    "mysql2": "npm:mysql2@3.14.0",
    "canvas": "https://deno.land/x/canvas@v1.4.2/mod.ts",
    "dayjs": "npm:dayjs@1.11.13",
    "js-md5": "npm:js-md5@0.8.3",
    "decimal.js": "https://esm.sh/decimal.js@10.5.0",
    "graphql": "npm:graphql@16.10.0",
    "mime": "npm:mime@4.0.6",
    "jose/": "https://deno.land/x/jose@v6.0.10/",
    "mnemonist": "npm:mnemonist@0.40.2",
    "redis": "https://deno.land/x/redis@v0.38.0/mod.ts",
    "sqlstring": "https://deno.land/x/sail_sqlstring@v1.0.0/mod.ts",
    "mqtt": "jsr:@ymjacky/mqtt5@0.0.19",
    "wechatpay-node-v3": "npm:wechatpay-node-v3@2.2.1"
  }
}
