{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^App(.*)": "@/components/App$1/App$1.vue",
      "^Custom(.*)": "@/components/Custom$1/Custom$1.vue",
      "^Dict(.*)": "@/components/Dict$1/Dict$1.vue",
      "^tm-(.*)": "@/uni_modules/tm-ui/components/tm-$1/tm-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "uni-app"
      }
    },
    {
      "path": "pages/index/Login",
      "style": {
        "navigationBarTitleText": "登录"
      }
    },
    {
      "path": "pages/dyn_page/List",
      "style": {
        "navigationBarTitleText": "动态页面列表"
      }
    },
    {
      "path": "pages/dyn_page/Detail",
      "style": {
        "navigationBarTitleText": "动态页面详情"
      }
    },
    {
      "path": "pages/dyn_page_field/List",
      "style": {
        "navigationBarTitleText": "动态页面字段列表"
      }
    },
    {
      "path": "pages/dyn_page_field/Detail",
      "style": {
        "navigationBarTitleText": "动态页面字段详情"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}
